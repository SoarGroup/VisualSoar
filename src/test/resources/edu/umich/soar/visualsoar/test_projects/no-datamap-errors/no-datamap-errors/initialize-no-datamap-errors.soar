sp {propose*initialize-no-datamap-errors
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-no-datamap-errors)
}

sp {apply*initialize-no-datamap-errors
   (state <s> ^operator <op>)
   (<op> ^name initialize-no-datamap-errors)
-->
   (<s> ^name no-datamap-errors)
}

